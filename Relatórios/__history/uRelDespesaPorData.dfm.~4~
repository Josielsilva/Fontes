  object QryDespesa: TZQuery
    Connection = DtmPrincipal.ConexaoDB
    SQL.Strings = (
      'SELECT Desp_Cod'
      '      ,Desp_Tipo'
      '      ,Desp_Descricao'
      '      ,Desp_dataVencimento'
      '      ,Desp_Valor'
      '      ,Desp_Status'
      '  FROM DESPESA DESP'
      ''
      
        '   WHERE DESP.DESP_DataVencimento BETWEEN :DataInicio And :DataF' +
        'im'
      #9' ORDER BY DESP.DESP_DataVencimento'
      ''
      '')
    Params = <
      item
        DataType = ftUnknown
        Name = 'DataInicio'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataFim'
        ParamType = ptUnknown
      end>
    Left = 520
    Top = 432
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DataInicio'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataFim'
        ParamType = ptUnknown
      end>
    object QryDespesaDesp_Cod: TIntegerField
      FieldName = 'Desp_Cod'
      ReadOnly = True
    end
    object QryDespesaDesp_Tipo: TIntegerField
      FieldName = 'Desp_Tipo'
    end
